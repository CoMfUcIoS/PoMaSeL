{
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": ".",
        "paths": {
            "utils/*": ["src/utils/*"],
            "controllers/*": ["src/controllers/*"],
            "middlewares/*": ["src/middlewares/*"],
            "models/*": ["src/models/*"],
            "routes/*": ["src/routes/*"],
        },
        "sourceMap": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
    }
}