{
  "name": "pomasel",
  "version": "1.0.0",
  "description": "Poor Man ServerLess",
  "main": "dist/server.js",
  "engines": {
    "node": ">=14.17.3",
    "npm": ">=7.20.0"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx}": [
      "eslint --fix --ext .js --ext .ts --ext .json"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "chai": "^4.3.4",
    "check-engine": "^1.10.1",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.62.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "scripts": {
    "preinstall": "check-engine",
    "postinstall": "husky install",
    "fix-lint-staged": "lint-staged",
    "build": "webpack --config src/webpack.config.ts --mode production",
    "start": "webpack serve  --config src/webpack.config.ts --mode development --devtool inline-source-map --hot",
    "lint": "eslint --ext .js --ext .ts --ext .json src",
    "fix-lint": "eslint --fix --ext .js --ext .ts --ext .json src",
    "test": "nyc mocha \"./src/**/*.test.{js,ts}\" --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoMfUcIoS/PoMaSeL.git"
  },
